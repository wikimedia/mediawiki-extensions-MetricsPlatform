{
	"opts": {
		"destination": "docs",
		"package": "package.json",
		"pedantic": false,
		"readme": "README.md",
		"recurse": true,
		"template": "node_modules/jsdoc-wmf-theme"
	},
	"plugins": [
		"node_modules/jsdoc-wmf-theme/plugins/default"
	],
	"source": {
		"include": [
			"modules"
		],
		"exclude": [
			"modules/ext.xLab/Experiments.js"
		]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		},
		"wmf": {
			"repository": "https://gerrit.wikimedia.org/g/mediawiki/extensions/MetricsPlatform/",
			"linkMap": {
				"mw.eventLog.submitInteraction": "https://doc.wikimedia.org/EventLogging/master/js/mw.eventLog.html#.submitInteraction"
			}
		}
	}
}
